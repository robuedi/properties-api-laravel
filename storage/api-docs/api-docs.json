{
    "openapi": "3.0.0",
    "info": {
        "title": "Properties API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/auth/register": {
            "post": {
                "tags": [
                    "authentication"
                ],
                "summary": "Register",
                "description": "Register user",
                "operationId": "v1-auth-register",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "Eduard"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "email",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "email"
                        },
                        "example": "eduard.test@gmail.com"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "password",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "password"
                        },
                        "example": "abcdef"
                    },
                    {
                        "name": "password_confirmation",
                        "in": "query",
                        "description": "password confirmation",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "password"
                        },
                        "example": "abcdef"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/auth/login": {
            "post": {
                "tags": [
                    "authentication"
                ],
                "summary": "Login",
                "description": "Login user",
                "operationId": "v1-auth-login",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "email",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "email"
                        },
                        "example": "eduard.test@gmail.com"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "password",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "password"
                        },
                        "example": "abcdef"
                    },
                    {
                        "name": "device_name",
                        "in": "query",
                        "description": "device name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "web"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/auth/logout": {
            "get": {
                "tags": [
                    "authentication"
                ],
                "summary": "Logout",
                "description": "Logout user",
                "operationId": "v1-auth-logout",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/auth/user": {
            "get": {
                "tags": [
                    "authentication"
                ],
                "summary": "User",
                "description": "get user",
                "operationId": "v1-auth-get-user",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/auth/refresh-token": {
            "get": {
                "tags": [
                    "authentication"
                ],
                "summary": "Refresh token",
                "description": "refresh token",
                "operationId": "v1-auth-refresh-token",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/cities": {
            "get": {
                "tags": [
                    "cities"
                ],
                "summary": "Cities",
                "description": "get cities",
                "operationId": "v1-cities-index",
                "parameters": [
                    {
                        "name": "filter",
                        "in": "query",
                        "description": "Filter the results",
                        "style": "deepObject",
                        "schema": {
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "example": "London"
                                },
                                "country_id": {
                                    "type": "integer",
                                    "example": "1"
                                }
                            },
                            "type": "object"
                        }
                    },
                    {
                        "name": "fields",
                        "in": "query",
                        "description": "Fields",
                        "style": "deepObject",
                        "schema": {
                            "properties": {
                                "cities": {
                                    "type": "string",
                                    "example": "id,name,country_id,created_at,updated_at"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/countries": {
            "get": {
                "tags": [
                    "countries"
                ],
                "summary": "Countries",
                "description": "get countries",
                "operationId": "v1-countries-index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/listing-types": {
            "get": {
                "tags": [
                    "listing types"
                ],
                "summary": "Listing types",
                "description": "get listing types",
                "operationId": "v1-listing-types-index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/properties": {
            "get": {
                "tags": [
                    "properties"
                ],
                "summary": "Properties",
                "description": "get properties",
                "operationId": "v1-properties-index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/v1/property-types": {
            "get": {
                "tags": [
                    "property types"
                ],
                "summary": "Property types",
                "description": "get property types",
                "operationId": "v1-property-types-index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
                "description": "Login with email, password and token to get the authentication token",
                "name": "Token based auth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}